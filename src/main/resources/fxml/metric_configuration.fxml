<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="600.0" prefWidth="800.0" spacing="10.0" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="10.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#goBack" />
            <Label fx:id="metric_name" text="[Metric Name]">
               <font>
                  <Font name="System Bold" size="25.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox prefWidth="200.0">
         <children>
            <Label fx:id="project_name" text="[Project Name]">
               <font>
                  <Font name="System Bold" size="21.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <TitledPane expanded="false" text="Parameters">
         <content>
            <ScrollPane prefHeight="200.0" prefWidth="660.0">
               <content>
                  <VBox spacing="10.0">
                     <children>
                        <VBox>
                           <children>
                              <Label text="Sprint">
                                 <font>
                                    <Font name="System Bold" size="13.0" />
                                 </font>
                              </Label>
                              <ComboBox fx:id="sprint_combobox" prefWidth="150.0" />
                           </children>
                        </VBox>
                        <VBox fx:id="parameters_root" />
                     </children>
                  </VBox>
               </content>
            </ScrollPane>
         </content>
      </TitledPane>
      <VBox VBox.vgrow="ALWAYS">
         <children>
            <ScrollPane fitToWidth="true" nodeOrientation="LEFT_TO_RIGHT" VBox.vgrow="ALWAYS">
               <content>
                  <VBox>
                     <children>
                        <Label text="Visualization">
                           <font>
                              <Font name="System Bold" size="21.0" />
                           </font>
                        </Label>
                        <VBox fx:id="visualization_root" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</fx:root>
